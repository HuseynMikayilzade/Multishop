@model UpdateSettingVm


<div class="col-md-6 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Create setting</h4>
            <form class="forms-sample" method="post" enctype="multipart/form-data">
                <div class="form-group">
                    <label asp-for="Key">Key</label>
                    <input asp-for="Key" type="text" class="form-control" id="exampleInputUsername1" placeholder="Key">
                    <span asp-validation-for="Key"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Value">Value</label>
                    <input asp-for="Value" type="text" class="form-control" id="exampleInputUsername1" placeholder="Value">
                    <span asp-validation-for="Value"></span>
                </div>
                <button type="submit" class="btn btn-primary me-2">Submit</button>
                <button class="btn btn-light"><a asp-action="index" asp-controller="setting">Cancel</a></button>

            </form>
        </div>
    </div>
</div>